{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },

    "parserOptions": {
        "ecmaVersion": 8
    },

    "rules": {
        "no-underscore-dangle": 0,

        "quotes": [2, "single"],
        "strict": [2, "global"],

        "consistent-return": 0,

        "space-in-parens": [2, "never"],
        "linebreak-style": [2, "unix"],
        
        "no-unused-vars" : [2, {"vars": "all", "args": "none"}],
        "no-console": "off"
    }
}